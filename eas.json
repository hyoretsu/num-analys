{
    "cli": {
        "appVersionSource": "remote",
        "version": ">= 3.8.1"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development"
        },
        "preview": {
            "distribution": "internal",
            "channel": "preview"
        },
        "production": {
            "channel": "main",
            "autoIncrement": true
        }
    },
    "submit": {
        "production": {
            "android": {
                "track": "production"
            }
        }
    }
}
