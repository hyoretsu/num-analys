{
	"name": "numanalysis",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"android": "expo run:android",
		"build": "expo prebuild --clean && prettier -w package.json",
		"ios": "expo run:ios",
		"prepare": "husky install",
		"preview:android": "eas build -p android --profile preview --local",
		"preview:ios": "eas build -p ios --profile preview --local",
		"release": "eas build -p all",
		"start": "expo start --tunnel",
		"start:android": "expo start --android --tunnel",
		"start:ios": "expo start --ios --tunnel",
		"start:prod": "expo start --dev-client --tunnel"
	},
	"lint-staged": {
		"**/*.(ts|tsx)": [
			"eslint --fix"
		]
	},
	"dependencies": {
		"@expo/metro-runtime": "^2.2.3",
		"@hyoretsu/rn-components": "^1.0.0",
		"expo": "~49.0.0",
		"expo-constants": "~14.4.2",
		"expo-linking": "~5.0.2",
		"expo-localization": "~14.3.0",
		"expo-modules-autolinking": "~1.5.0",
		"expo-router": "2.0.0",
		"expo-splash-screen": "~0.20.4",
		"expo-status-bar": "~1.6.0",
		"i18n-js": "^4.3.0",
		"numerical-methods": "^0.11.11",
		"react": "18.2.0",
		"react-native": "0.72.1",
		"react-native-gesture-handler": "~2.12.0",
		"react-native-safe-area-context": "4.6.3",
		"react-native-screens": "~3.22.0",
		"rn-css": "^1.10.3"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@rnx-kit/metro-resolver-symlinks": "^0.1.28",
		"@types/react": "~18.2.14",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-import-helpers": "^1.3.1",
		"eslint-plugin-prettier": "5.0.0-alpha.2",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-native": "^4.0.0",
		"husky": "^8.0.3",
		"jest": "^29.2.1",
		"jest-expo": "~49.0.0",
		"lint-staged": "^13.2.3",
		"metro-runtime": "^0.77.0",
		"prettier": "^3.0.0",
		"react-test-renderer": "18.2.0",
		"typescript": "^5.1.6"
	}
}
